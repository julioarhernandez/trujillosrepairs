---
  layout: layouts/default.njk
  title: Eleventy Template title
  eleventyNavigation:
    key: Home
    order: 0
---

<!-- home
================================================== -->
<section id="home" style="background-image: url('{{ home.home.header.backgroundImage }}');">

    <div class="overlay"></div>

    <div class="home-content-table">
        <div class="home-content-tablecell">
            <div class="row">
                <div class="col-twelve">
                    <h3 class="animate-intro">{{ home.home.header.subtitle }}</h3>
                    <h1 class="animate-intro">{{ home.home.header.title | safe }}</h1>
                    <div class="more animate-intro">
                        <a class="smoothscroll button stroke" href="{{ home.home.header.button.href }}">
                            {{ home.home.header.button.text }}
                        </a>
                    </div>
                </div> <!-- end col-twelve -->
            </div> <!-- end row -->
        </div> <!-- end home-content-tablecell -->
    </div> <!-- end home-content-table -->

    <ul class="home-social-list">
        {% for social in global.global.social %}
        <li class="animate-intro">
            <a href="{{ social.href }}"><i class="fa {{ social.icon }}"></i></a>
        </li>
        {% endfor %}
    </ul> <!-- end home-social-list -->

    <div class="scrolldown">
        <a href="#about" class="scroll-icon smoothscroll">
            Scroll Down
            <i class="fa fa-long-arrow-right" aria-hidden="true"></i>
        </a>
    </div>

</section> <!-- end home -->


<!-- about
================================================== -->
<section id="about">

    <div class="row about-wrap">
        <div class="col-full">

            <div class="about-profile-bg"></div>

            <div class="intro">
                <h3 class="animate-this">{{home.home.about.title }}</h3>
                <p class="lead animate-this">{{home.home.about.content }}</p>
            </div>

        </div> <!-- end col-full  -->
    </div> <!-- end about-wrap  -->

</section> <!-- end about -->


<!-- about
================================================== -->
<section id="services" style="background-image: url({{ home.home.services.backgroundImage }});" >

    <div class="overlay"></div>
    <div class="gradient-overlay"></div>

    <div class="row narrow section-intro with-bottom-sep animate-this">
        <div class="col-full">
            <h3>{{home.home.services.subtitle }}</h3>
            <h1>{{home.home.services.title }}</h1>
            <p class="lead">{{ home.home.services.content | safe }}</p>
        </div> <!-- end col-full -->
    </div> <!-- end row -->

    <div class="row services-content">
        <div class="services-list block-1-2 block-tab-full group">
            {% for service in home.home.services.items %}
            <div class="bgrid service-item animate-this">
                <span class="icon">
                    {% if service.fa == 'true' %}
                        <i class="{{ service.icon }}"></i>
                    {% else %}
                        {% myImage service.image, 'Our work', 'service_icon', service.resize %}
                    {% endif %}
                </span>
                <div class="service-content">
                    <h3 class="h05">{{ service.title }}</h3>
                    {{ service.content | safe }}
                </div>

            </div> <!-- end bgrid -->
            {% endfor %}

        </div> <!-- end services-list -->

    </div> <!-- end services-content -->

</section> <!-- end services -->


